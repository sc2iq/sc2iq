{
  "name": "sc2iq-service",
  "version": "1.0.0",
  "description": "Service for Sc2iq",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node ./build/index.js",
    "watch": "concurrently --kill-others -p [{name}-{pid}] -n tsc,service \"tsc -w\" \"nodemon\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate": "gql-gen --config codegen.yml"
  },
  "keywords": [
    "starcraft",
    "sc2",
    "sc2iq",
    "intelligence",
    "quotient"
  ],
  "author": "Matt Mazzola <mattmazzola@live.com> (https://github.com/mattmazzola)",
  "license": "MIT",
  "files": [
    "build",
    "src"
  ],
  "dependencies": {
    "apollo-server": "^2.8.1",
    "dotenv": "^8.1.0",
    "graphql": "^14.4.2",
    "mongodb": "^3.3.0",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/mongodb": "^3.2.3",
    "@types/uuid": "^3.4.5",
    "graphql-code-generator": "^0.18.2",
    "graphql-codegen-typescript-common": "^0.18.2",
    "graphql-codegen-typescript-resolvers": "^0.18.2",
    "graphql-codegen-typescript-server": "^0.18.2",
    "lodash": "^4.17.21",
    "nodemon": "^1.19.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "nodemonConfig": {
    "watch": [
      "build",
      ".env"
    ],
    "ignore": [
      "generated"
    ],
    "delay": "1000"
  }
}
